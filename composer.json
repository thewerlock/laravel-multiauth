{
    "name": "thewerlock/laravel-multiauth",
    "description": "Laravel Multiauth package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel multiauth",
        "multiauth",
        "thewerlock",
        "admin auth",
        "multi auth"
    ],
    "authors": [{
        "name": "pradeep-thakur",
        "email": "pradeepthakur@outlook.com"
    }],
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Thewerlock\\Multiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thewerlock\\Multiauth\\Tests\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thewerlock\\Multiauth\\MultiauthServiceProvider"
            ]
        }
    }
}
